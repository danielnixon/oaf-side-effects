{
  "name": "oaf-side-effects",
  "version": "3.0.0",
  "main": "dist",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oaf-project/oaf-side-effects.git"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^3.2.3",
    "@stryker-mutator/jest-runner": "^3.2.4",
    "@stryker-mutator/typescript": "^3.2.3",
    "@types/jest": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/experimental-utils": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "codecov": "^3.6.5",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-sonarjs": "^0.5.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "type-coverage": "^2.6.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write '{src,test}/**/*.{ts,tsx}'",
    "documentation": "typedoc --excludeNotExported --excludePrivate --out docs src && cp docs_config.yml ./docs/_config.yml",
    "release": "yarn build && yarn lint && yarn type-coverage --strict --ignore-catch --detail && yarn format-lint && yarn test && yarn publish && yarn documentation",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "typeCoverage": {
    "atLeast": 100
  }
}
